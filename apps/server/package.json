{
  "name": "server",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.6.3",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json --resolve-full-paths --resolve-full-extension .js",
    "test": "vitest run",
    "test:all": "vitest run --config vitest.config.ts --mode test",
    "test:unit": "vitest run --config vitest.config.ts --mode test --exclude '**/*.int.test.ts'",
    "test:watch": "vitest",
    "test:int": "vitest run --config vitest.int.config.ts --mode test",
    "unit:test": "pnpm test:unit",
    "int:test": "pnpm test:int",
    "dev:test": "vitest --watch",
    "lint": "eslint .",
    "dev": "tsx --watch src/http/server.ts",
    "worker": "tsx src/worker/index.ts",
    "worker:dispatch-once": "tsx src/worker/commands/dispatch-outbox-once.ts",
    "worker:dlq-once": "tsx src/worker/commands/process-dlq-once.ts",
    "worker:email-once": "tsx src/worker/commands/process-email-once.ts",
    "worker:fixed-slot-assign-once": "tsx src/worker/commands/fixed-slot-assign-once.ts",
    "worker:subscriptions-expire-sweep-once": "tsx src/worker/commands/subscriptions-expire-sweep-once.ts",
    "worker:wallet-withdrawal-sweep-once": "tsx src/worker/commands/wallet-withdrawal-sweep-once.ts",
    "start": "node dist/src/http/server.js",
    "seed": "pnpm prisma db seed",
    "seed:bikes": "tsx prisma/seed-bikes.ts",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@date-fns/tz": "^1.4.1",
    "@hono/node-server": "^1.19.3",
    "@hono/zod-openapi": "^1.1.3",
    "@mebike/shared": "workspace:*",
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "@scalar/hono-api-reference": "^0.9.19",
    "@types/nodemailer": "^7.0.3",
    "bcrypt": "^6.0.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^17.2.3",
    "effect": "^3.19.10",
    "hono": "^4.9.4",
    "ioredis": "5.8.2",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.28.9",
    "nodemailer": "^6.10.1",
    "pg": "^8.16.3",
    "pg-boss": "^12.5.4",
    "pino": "^8.21.0",
    "prisma-kysely": "^2.2.1",
    "stripe": "^20.1.0",
    "uuidv7": "^1.1.0",
    "zod": "^4.3.5",
    "expo": "~54.0.31",
    "react": "19.1.0",
    "react-native": "0.81.5"
  },
  "devDependencies": {
    "@mebike/eslint-config": "workspace:*",
    "@mebike/tsconfig": "workspace:*",
    "@testcontainers/postgresql": "^11.10.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.16.0",
    "eslint": "^9.36.0",
    "pino-pretty": "^9.4.1",
    "prisma": "^7.1.0",
    "testcontainers": "^11.10.0",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.5",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  },
  "prisma": {
    "seed": "pnpm tsx prisma/seed.ts",
    "schema": "./prisma"
  }
}
