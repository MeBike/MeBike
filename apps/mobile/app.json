{
  "expo": {
    "name": "mobile",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSBluetoothPeripheralUsageDescription": "Allow MeBike to connect to your bike",
        "NSBluetoothAlwaysUsageDescription": "Allow MeBike to connect to your bike",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.aaang123.mobile"
    },
    "android": {
      "package": "com.aaang123.mobile",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "fe3621a0-88f9-4578-b1a7-e989bdb70c86"
      }
    }
  }
}
