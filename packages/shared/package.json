{
  "name": "@mebike/shared",
  "version": "0.1.0",
  "packageManager": "pnpm@10.6.3",
  "description": "Shared schemas and types for MeBike apps.",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
