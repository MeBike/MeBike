{
  "extends": "@mebike/tsconfig/base.json",
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./dist/.tsbuildinfo",
    "rootDir": "./src",
    "module": "commonjs",
    "moduleResolution": "node10",
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": false,
    "outDir": "./dist"
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts"
  ],
  "exclude": [
    "dist"
  ]
}
